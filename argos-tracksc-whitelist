# This whitelist contains the modules and the functions exported by the module
# that are allowed to execute.
[kernel32.dll]
LoadLibraryA
LoadLibraryExA
LoadLibraryW
LoadLibraryExW
# download_exec payload
GetProcAddress
GetSystemDirectoryA
#CreateProcessA
#WaitForSingleObject
[ws2_32.dll]
WSAStartup
WSASocketA
bind
listen
accept
closesocket
[urlmon.dll]
URLDownloadToFileA
